{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "pnpm install --shamefully-hoist",
  "env": {
    "NODE_ENV": "production",
    "NPM_CONFIG_REGISTRY": "https://registry.npmjs.org"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
